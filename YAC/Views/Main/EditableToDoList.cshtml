@model ToDoListModel

<div id="todolist-@Model.Id" class="border rounded shadow p-2">
    <h3 class="text-xl font-bold mb-4 border-b">@Model.Title</h3>
    <ul>
        @foreach(var item in Model.Items)
        {
            <partial name="EditableToDoItem" for="@item" />
        }
    </ul>   
</div>